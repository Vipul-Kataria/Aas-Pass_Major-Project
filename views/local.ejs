<%layout("./layout/boilerplate.ejs")%>



<body class="new-body">
    <br>
    <h1 class="city-head">Want to find events Aas-Pass hmmm..? Choose your city...</h1>
    <br>
    <div class="showLocal-main row-10 offset-1">
    <%for (city of cities){%>
        <div class="city-div col-10">
            <form action="/local/<%city%>" method="get">
                <a href="/local/<%=city%>" class="text-of-local"><%=city%></a>
            </form>
        </div>
    <%}%>
</div>
</body>